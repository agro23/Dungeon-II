@{
  Layout = "_Layout";
}
<div class = "container well-gametop text-center">
  @if(@Model["item"].Count > 0)
  {
    @foreach(var items in @Model["item"])
    {
      <h4><a href="#">@items.GetName()</a> | <a href="/game/select/@Model["pc"].GetId()/@Model["room"].GetId()/@items.GetId()">GET</a> | <a href="/game">BACK</a> </h4>
    }
  }
  else{
  <h4> There Are No Items To Get| <a href="/game">BACK</a> </h4>
}
</div>
